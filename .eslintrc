{
    "parser": "@typescript-eslint/parser",
    "overrides": [
        {
            "files": [
                "*.ts",
                "*.tsx"
            ],
            "parserOptions": {
                "project": [
                    "./tsconfig.json"
                ]
            }
        }
    ],
    "extends": [
        "airbnb",
        "airbnb-typescript"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "jsx-a11y/click-events-have-key-events": "off",
        "react/require-default-props": "off",
        "jsx-a11y/no-static-element-interactions": "off",
        "react/no-unused-prop-types": "off",
        "no-unused-vars":"off",
        "no-debugger":"off"
    }
}