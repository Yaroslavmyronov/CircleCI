{
    "parser": "@typescript-eslint/parser",
    "overrides": [
        {
            "files": [
                "*.ts",
                "*.tsx"
            ],
            "parserOptions": {
                "project": [
                    "./tsconfig.json"
                ]
            }
        }
    ],
    "extends": [
        "airbnb",
        "airbnb-typescript"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {}
}